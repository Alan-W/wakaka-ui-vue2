{
  "name": "wakak-ui",
  "version": "1.0.0",
  "main": "index.js",
  "author": "大玉子",
  "license": "MIT",
  "files": [
    "src",
    "lib",
    "packages"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint:lint-staged": "int-staged",
    "clean": "rimraf lib",
    "serve:demo": "npm run dist && rollup -c ./build/rollup.demo.js -w",
    "build:file": "node ./build/bin/build-entry.js",
    "build:theme": "node ./build/bin/gencssfile.js && gulp build --gulpfile packages/theme/gulpfile.js && cp-cli packages/theme/lib lib/theme",
    "build:lib": "rollup -c ./build/rollup.config.js",
    "dist": "npm run clean && npm run build:file && npm run build:lib && npm run build:theme"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-url": "^7.0.0",
    "@vue/compiler-sfc": "^3.2.37",
    "json-templater": "^1.2.0",
    "tslib": "^2.4.0",
    "vue": "^2.7.8"
  },
  "peerDependencies": {
    "vue": "^2.7.8"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-html": "^0.2.4",
    "babel": "^6.23.0",
    "cp-cli": "^2.0.0",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-vue": "^6.0.0",
    "rollup-plugin-vue2": "^0.8.1",
    "uppercamelcase": "^3.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "ie > 9"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
